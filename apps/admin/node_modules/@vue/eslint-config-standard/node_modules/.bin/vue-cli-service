#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/@vue+cli-service@5.0.8_@vue+compiler-sfc@3.5.13_lodash@4.17.21_sass-loader@12.6.0_sass@1.85.1_4kxtggqzjmfkwhl2okzlhu3dh4/node_modules/@vue/cli-service/bin/node_modules:/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/@vue+cli-service@5.0.8_@vue+compiler-sfc@3.5.13_lodash@4.17.21_sass-loader@12.6.0_sass@1.85.1_4kxtggqzjmfkwhl2okzlhu3dh4/node_modules/@vue/cli-service/node_modules:/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/@vue+cli-service@5.0.8_@vue+compiler-sfc@3.5.13_lodash@4.17.21_sass-loader@12.6.0_sass@1.85.1_4kxtggqzjmfkwhl2okzlhu3dh4/node_modules/@vue/node_modules:/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/@vue+cli-service@5.0.8_@vue+compiler-sfc@3.5.13_lodash@4.17.21_sass-loader@12.6.0_sass@1.85.1_4kxtggqzjmfkwhl2okzlhu3dh4/node_modules:/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/@vue+cli-service@5.0.8_@vue+compiler-sfc@3.5.13_lodash@4.17.21_sass-loader@12.6.0_sass@1.85.1_4kxtggqzjmfkwhl2okzlhu3dh4/node_modules/@vue/cli-service/bin/node_modules:/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/@vue+cli-service@5.0.8_@vue+compiler-sfc@3.5.13_lodash@4.17.21_sass-loader@12.6.0_sass@1.85.1_4kxtggqzjmfkwhl2okzlhu3dh4/node_modules/@vue/cli-service/node_modules:/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/@vue+cli-service@5.0.8_@vue+compiler-sfc@3.5.13_lodash@4.17.21_sass-loader@12.6.0_sass@1.85.1_4kxtggqzjmfkwhl2okzlhu3dh4/node_modules/@vue/node_modules:/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/@vue+cli-service@5.0.8_@vue+compiler-sfc@3.5.13_lodash@4.17.21_sass-loader@12.6.0_sass@1.85.1_4kxtggqzjmfkwhl2okzlhu3dh4/node_modules:/mnt/d/myproject/毕业设计/pet-mall/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../cli-service/bin/vue-cli-service.js" "$@"
else
  exec node  "$basedir/../../../cli-service/bin/vue-cli-service.js" "$@"
